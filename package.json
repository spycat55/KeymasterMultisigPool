{
  "name": "keymaster-multisig-pool",
  "version": "0.1.0",
  "description": "A multisig pool implementation with TypeScript and Go support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "types",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:go": "go build -o build/ ./cmd/...",
    "build:all": "npm run build && npm run build:go",
    "dev": "tsup --watch",
    "test": "jest",
    "test:go": "go test ./...",
    "test:all": "npm run test && npm run test:go",
    "lint": "eslint src/**/*.ts",
    "lint:go": "golangci-lint run",
    "clean": "rm -rf dist build",
    "prepublishOnly": "npm run build:all",
    "publish:npm": "npm publish",
    "publish:go": "./scripts/publish-go.sh",
    "publish:all": "npm run publish:npm && npm run publish:go"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "@bsv/sdk": "^1.6.12"
  },
  "keywords": [
    "multisig",
    "pool",
    "blockchain",
    "typescript",
    "golang"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spycat55/KeymasterMultisigPool.git"
  },
  "author": "spycat55",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
